      "source": [
        "for number in range(1,201):\n",
        "  if number>1:\n",
        "    for i in range (2,number):\n",
        "      if number%i==0:\n",
        "        break\n",
        "    else:\n",
        "      print(number,end=\",\")       \n",
        "\n",
        "\n"
